{
  "env": {
    "browser": false,
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "semi": ["error", "never"],
    "no-cond-assign": "error",
    "no-constant-condition": "error",
    "no-dupe-args": "error",
    "no-dupe-keys": "error",
    "no-duplicate-case": "error",
    "no-extra-boolean-cast": "error",
    "no-inner-declarations": "error"
  },
  "plugins": [
    "jest"
  ],
  "extends": [
    "eslint:recommended"
  ],
  "overrides": [
  ]
}